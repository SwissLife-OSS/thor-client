{
  "Tracing": {
    "ApplicationId": "432",
    "Level": "Informational",
    "EventHub": {
      "TransportType": "AmqpWebSockets"
    },
    "ApplicationRootPath": null,
    "InProcess": "true",
    "Enabled": "true",
    "Forwarding": [
      {
        "Regex": "Microsoft.AspNetCore.Hosting.Internal.WebHost",
        "Level": "Warning"
      },
      {
        "Regex": "Microsoft.*",
        "Level": "Info"
      }
    ],
    "AllowedPrefixes": [
      "Thor."
    ],
    "SkipRequestFilter": "_health",
    "Events": {
      "Buffer": {
        "Size": 2000,
        "EnqueueTimeout": "0:00:00:00.500",
        "DequeueBatchSize": 500
      },
      "Storage": {
        "DequeueBatchSize": 500
      }
    },
    "Attachments": {
      "Buffer": {
        "Size": 500,
        "EnqueueTimeout": "-0:00:00:00.001",
        "DequeueBatchSize": 500
      },
      "Storage": {
        "DequeueBatchSize": 500
      }
    }
  }
}
